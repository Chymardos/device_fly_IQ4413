/boot		emmc	/dev/bootimg		flags=display="Boot";backup=1
/recovery	emmc	/dev/recovery		flags=display="Recovery";backup=1
/lk		emmc	/dev/uboot		flags=display="Lk";backup=1
/nvram		emmc	/dev/nvram		flags=display="Nvram";backup=1
/misc		emmc	/dev/misc		flags=display="Misc";backup=1
/logo		emmc	/dev/logo		flags=display="Logo";backup=1
/protect_f	ext4	/dev/block/mmcblk0p2	flags=display="Protect_f";backup=0
/protect_s	ext4	/dev/block/mmcblk0p3 	flags=display="Protect_s";backup=0
/sec_ro		emmc	/dev/block/mmcblk0p4	flags=display="Sec_ro";backup=0
/frp            emmc    /dev/block/mmcblk0p5    flags=display="FRP";backup=0
